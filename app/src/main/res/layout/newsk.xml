<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/boutonRetour5"
        android:layout_width="67dp"
        android:layout_height="86dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="4dp"
        android:onClick="onClickRetour"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/bouton_retour"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageNews1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="105dp"
        android:layout_marginBottom="16dp"
        android:onClick="onClickNews1"
        app:layout_constraintBottom_toTopOf="@+id/imageNews2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/news1" />

    <ImageView
        android:id="@+id/imageNews2"
        android:layout_width="393dp"
        android:layout_height="151dp"
        android:layout_marginTop="267dp"
        android:layout_marginBottom="20dp"
        android:onClick="onClickNews2"
        app:layout_constraintBottom_toTopOf="@+id/imageNews3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/news2" />

    <ImageView
        android:id="@+id/imageNews3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="135dp"
        android:onClick="onClickNews3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/news3" />
</androidx.constraintlayout.widget.ConstraintLayout>